# Data:
# 010 : 0
# 0110 : 1
# 01110 : dead <-> active
# 011110 : active <-> program

# Markers:
# 000 : initial
# 0101110 : moving towards the program
# 010110 : moving a command to the end
# 010111111110 : moving a command to the start
# 01011110 : moving to execute 1
# 010111110 : ending

# Initial state
000 = 01110 0110 0101110 011110


# Change depending on what is in front and move to end of program
0101110 011110 010 = 011110 010110 010
0101110 011110 0110 = 011110 010110 0110

(010110 010) 010 = 010 (010110 010)
(010110 010) 0110 = 0110 (010110 010)
(010110 0110 010) 010 = 010 (010110 0110 010)
(010110 0110 010) 0110 = 0110 (010110 0110 010)
(010110 0110 0110) 010 = 010 (010110 0110 0110)
(010110 0110 0110) 0110 = 0110 (010110 0110 0110)


# Get to the start
(010110 010) = (010111111110 010) 010
(010110 0110 010) = (010111111110 0110 010) 0110 010
(010110 0110 0110) = (010111111110 0110 0110) 0110 0110

010 (010111111110 010) = (010111111110 010) 010
0110 (010111111110 010) = (010111111110 010) 0110
011110 (010111111110 010) = (010111111110 010) 011110
010 (010111111110 0110 010) = (010111111110 0110 010) 010
0110 (010111111110 0110 010) = (010111111110 0110 010) 0110
011110 (010111111110 0110 010) = (010111111110 0110 010) 011110
010 (010111111110 0110 0110) = (010111111110 0110 0110) 010
0110 (010111111110 0110 0110) = (010111111110 0110 0110) 0110
011110 (010111111110 0110 0110) = (010111111110 0110 0110) 011110


# Apply zero
01110 (010111111110 010) 010 = 010 01110 0101110
01110 (010111111110 010) 0110 = 0110 01110 0101110

# One: false case
(010111111110 0110 010) 010 = 0101110 010
(010111111110 0110 0110) 010 = 0101110 010

# One: true case
(010111111110 0110 010) = (010 01011110)
(010111111110 0110 0110) = (0110 01011110)

(010 01011110) 010 = 010 (010 01011110)
(010 01011110) 0110 = 0110 (010 01011110)
(0110 01011110) 010 = 010 (0110 01011110)
(0110 01011110) 0110 = 0110 (0110 01011110)

01011110 = 0101110


# Get back to starting state
0101110 010 = 010 0101110
0101110 0110 = 0110 0101110


# end
01110 0101110 011110 = 010111110
010111110 010 = 010111110
010111110 0110 = 010111110
010111110 = done
